<div class="modal-overlay">
  <div class="modal">
    <div class="close-modal">
      <i class="material-icons">
        close
      </i>
    </div>

    <div class="maximize-modal">
      <i class="material-icons">
        open_in_full
      </i>
    </div>

    <div class="modal-content">
      <iframe src="" frameborder="0"></iframe>
    </div>
    

  </div>
</div>
<script src="/scripts.js"></script>
{% endblock %}